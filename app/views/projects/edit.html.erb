<h3> Edit project </h3>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@project) do |f| %>

		
      
			<%= f.label :name%>
			<%= f.text_field :name %>

			<%= f.label :code %>
			<%= f.text_field :code %>

			<%= f.label :description %>
			<%= f.text_field :description %>

			<%= f.label :billing_type %>
			<%= f.select :billing_type,options_for_select(%w[tnm fixed]) %>

			<%= f.label :start_date %>
			<%= f.date_field :start_date %>

			<%= f.label :deadline_date%>
			<%= f.date_field :dead_date %>

			<%= f.label :end_date %>
			<%= f.date_field :end_date %>

			<%= f.label :github_url%>
			<%= f.text_field :github_url%>
			
			<%= f.label :status %>
			<%= f.select :status,options_for_select(%w[poc kick_off completed]) %>

			
			

			<%= f.submit "Add project", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>