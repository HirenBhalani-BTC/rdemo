<h3><%= link_to "Add new project",new_path%></h3>

<div style="padding:30px 30px 30px 0px;">

<table class="table table-striped table-bordered table-condensed">
	<tr> 
	<th>name</th>
	<th>code</th>
	<th>description</th>
	<th>billing type</th>
	<th>start date</th>
	<th>deadline</th>
	<th>end date</th>
	<th>github url</th>
	<th>status</th>
	<th>Action</th>
	</tr>
	<% @project.each do |p| %> 
	<tr>
	<td><%= link_to p.name, project_path(p) %></td>
	<td><%= p.code %></td>
	<td><%= p.description %></td>
	<td><%= p.billing_type %></td>
	<td><%= p.start_date %></td>
	<td><%= p.dead_date %></td>
	<td><%= p.end_date %></td>
	<td><%= p.github_url %></td>
	<td><%= p.status %></td>
	<td><%= link_to "Edit", edit_project_path(p) %>/<%= link_to "Delete",project_path(p),method: :delete,data:{confirm:"Are you Sure??"}%></td>
	</tr>
	<% end %>


</table>


</div>